{
  "name": "@soundws/audio-mix-service",
  "version": "3.0.1-beta.3",
  "description": "A microservice that mixes/merges multiple audio files into a single file with different volumes for each audio file",
  "license": "AGPL-3.0-or-later",
  "author": "The First Coders Team (https://www.firstcoders.co.uk)",
  "email": "support@firstcoders.co.uk",
  "bugs": "https://github.com/sound-ws/audio-mix-service/issues",
  "homepage": "https://www.sound.ws/products/stem-player",
  "contributors": [
    {
      "name": "Mark Cremer",
      "email": "mark@firstcoders.co.uk",
      "url": "http://www.firstcoders.co.uk"
    }
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sound-ws/audio-mix-service.git"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.22.20",
    "@babel/register": "^7.22.15",
    "@babel/runtime": "^7.22.15",
    "assert": "^2.0.0",
    "aws-sam-webpack-plugin": "^0.13.0",
    "aws-sdk": "^2.1463.0",
    "axios": "^0.26.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "core-js": "^3.32.2",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jsonschema": "^1.2.4",
    "jsonwebtoken": "^9.0.2",
    "license-webpack-plugin": "^4.0.2",
    "lint-staged": "^13.0.3",
    "mocha": "^9.2.1",
    "prettier": "^3.0.3",
    "sinon": "^13.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "uuid": "^8.3.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "widdershins": "^4.0.1"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha test/spec/**/**/*.js",
    "build": "npm run build:docs && npm run build:dist && sam build",
    "build:dist": "rm -rf .aws-sam dist && webpack --mode production && cp -R .aws-sam/build dist",
    "build:docs": "rimraf docs/ && mkdir docs/ && npx widdershins --environment widdershins.json openapi.yaml -o docs/api.md",
    "prepublishOnly": "npm run build",
    "qa": "eslint \"src/**/*.js\"",
    "precommit": "lint-staged",
    "sam:publish": "sam package --template-file .aws-sam/build/template.yaml --output-template-file .aws-sam/build/packaged.yaml --s3-bucket $AWS_SAM_PACKAGE_BUCKET_NAME --region ${AWS_REGION:=us-east-1} && sam publish --template .aws-sam/build/packaged.yaml --region ${AWS_REGION:=us-east-1} --semantic-version $npm_package_version"
  }
}
